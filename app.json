{
  "expo": {
    "name": "sacramap",
    "slug": "sacramap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdfdff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "BEFORE-DEPLOY-CHANGE-TO-GOOGLE-API-KEY"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fdfdff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
